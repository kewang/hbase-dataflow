.modal.fade#create-qualifiers-dialog(tabindex="-1", role="dialog", aria-labelledby="create-qualifiers-dialog-title", aria-hidden=true)
  .modal-dialog
    .modal-content
      .modal-header
        button.close(type="button", data-dismiss="modal", aria-hidden=true) &times;
        h4.modal-title#create-qualifiers-dialog-title Create {{ selectTable2.name }}
      .modal-body
        input.form-control(type="text", placeholder="Please Input Row Key")
        button.btn.btn-default(type="button", ng-click="addCQ()") Add CQ
        table.table
          tr
            th CQ name
            th Value
          tr
            td
              input.form-control(type="text")
            td
              input.form-control(type="text")
      .modal-footer
        button.btn.btn-default(type="button", data-dismiss="modal") Close
        button.btn.btn-primary(type="button") Save changes
