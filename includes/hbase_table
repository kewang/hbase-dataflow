
<table class="table table-bordered table-condensed table-hover">
  <tr>
    <th>Row Key</th>
    <th ng-repeat="column in columns">{{ column.getName() }}</th>
  </tr>
  <tr ng-repeat="row in rows">
    <th>{{ row.key }}</th>
    <td ng-repeat="value in row.values track by $index">{{ value.getValue() }}</td>
  </tr>
</table>